<?xml version="1.0"?>
<!--33) Definir vista de estate.property.tag-->
<odoo>
    <data>
        <!--33)a) Vista AcciÃ³n-->
        <!-- AcciÃ³n que abre la vista-->
        <record id="action_estate_property_tag" model="ir.actions.act_window">
            <field name="name">Etiquetas de propiedad</field>
            <field name="res_model">estate.property.tag</field>
            <field name="view_mode">list</field>
        </record>

        <!--33)b) Vista Lista con editable="top"-->
        <!-- Vista de tipo list -->
        <record id="custom_view_estate_property_tag_list" model="ir.ui.view">
            <field name="name">estate.property.tag.list</field>
            <field name="model">estate.property.tag</field>
            <field name="type">list</field>
            <field name="arch" type="xml">
                <list string="Etiquetas" editable="top">
                    <field name="name"/>
                </list>
            </field>
        </record>
    </data>
</odoo>