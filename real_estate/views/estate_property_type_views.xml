<?xml version="1.0"?>
<odoo>
    <data>
        <!-- AcciÃ³n que abre la vista-->
        <!--33)c) Dejar solo view_mode: list-->
        <record id="action_estate_property_type" model="ir.actions.act_window">
            <field name="name">Tipos de propiedad</field>
            <field name="res_model">estate.property.type</field>
            <field name="view_mode">list</field>
        </record>

        <!--33)c) Vista Lista con editable="top"-->
        <!-- Vista de tipo list -->
        <record id="custom_view_estate_property_type_list" model="ir.ui.view">
            <field name="name">estate.property.type.list</field>
            <field name="model">estate.property.type</field>
            <field name="type">list</field>
            <field name="arch" type="xml">
                <list string="Tipos" editable="top">
                    <field name="name"/>
                </list>
            </field>
        </record>
    </data>
</odoo>